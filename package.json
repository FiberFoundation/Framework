{
  "name": "fiber-framework",
  "version": "0.0.3",
  "description": "Scalable Event and Meta Driven Framework for Humans",
  "main": "dist/fiber.js",
  "scripts": {
    "prod": "webpack --mode=prod-build",
    "build": "webpack --mode=build",
    "dev": "npm run build -- --progress --colors --watch",
    "start": "npm run serve",
    "test": "jest",
    "check": "flow",
    "check:all": "flow check --all",
    "clean": "rm -rf ./dist && rm -rf ./docs",
    "example": "webpack-dev-server --mode=test --inline --content-base ./examples",
    "example:hot": "npm run serve -- --hot",
    "doc": "mr-doc -s ./src -o ./docs/doc -n 'Fiber' -t 'cayman'",
    "esdocs": "esdoc -c esdoc.json",
    "postinstall": "npm run build && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiberFoundation/Framework.git"
  },
  "keywords": [
    "js",
    "es6",
    "framework",
    "virtual dom",
    "immutable",
    "events",
    "model",
    "view",
    "collection",
    "router",
    "contracts",
    "ioc",
    "inversion of control",
    "di container",
    "client",
    "ui"
  ],
  "author": "Igor Krimerman <i.m.krimerman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FiberFoundation/Framework/issues"
  },
  "homepage": "https://github.com/FiberFoundation/Framework#readme",
  "dependencies": {
    "dataloader": "^1.2.0",
    "immutable": "^3.8.1",
    "lodash": "^4.12.0",
    "logatim": "^0.9.0",
    "ractive": "^0.7.3",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "iflow-lodash": "^1.1.17",
    "jest-cli": "^13.0.0",
    "jscs": "^3.0.4",
    "jscs-loader": "^0.3.0",
    "mr-doc": "^3.2.1",
    "mr-doc-theme-cayman": "^1.4.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.7.1"
  }
}
