{
  "name": "fiber-framework",
  "version": "0.0.3",
  "description": "Fiber brings simplicity and scalability to your JS Apps.",
  "main": "dist/fiber.js",
  "scripts": {
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "start": "npm run serve",
    "stop": "npm run clean",
    "test": "jest",
    "clean": "rm -rf ./dist && rm -rf ./docs",
    "example": "webpack-dev-server --mode=test --inline --content-base ./examples",
    "example:hot": "npm run serve -- --hot",
    "min": "uglifyjs ./dist/fiber.js --mangle --source-map ./dist/fiber.min.map -o ./dist/fiber.min.js",
    "doc": "mr-doc -s ./src -o ./docs/doc -n 'Fiber' -t 'cayman'",
    "esdocs": "esdoc -c esdoc.json",
    "postinstall": "npm run build && npm run min && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiberFoundation/Framework.git"
  },
  "keywords": [
    "js",
    "framework",
    "virtual dom",
    "events",
    "model",
    "view",
    "collection",
    "router",
    "contracts",
    "ioc",
    "inversion of control",
    "di container",
    "client",
    "ui"
  ],
  "author": "Igor Krimerman <i.m.krimerman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FiberFoundation/Framework/issues"
  },
  "homepage": "https://github.com/FiberFoundation/Framework#readme",
  "dependencies": {
    "core-js": "^2.4.0",
    "dataloader": "^1.2.0",
    "immutable": "^3.8.1",
    "lodash": "^4.12.0",
    "ractive": "^0.7.3",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-importpath-plugin": "0.0.1",
    "jest-cli": "^13.0.0",
    "jscs": "^3.0.4",
    "jscs-loader": "^0.3.0",
    "mr-doc": "^3.2.1",
    "mr-doc-theme-cayman": "^1.4.1",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.7.1"
  }
}
